{
  "name": "postit",
  "version": "1.0.0",
  "description": "A simple application that allows friends and colleagues create groups for notifications",
  "main": "./server/bin/www",
  "scripts": {
    "db:migrate": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "watch": "webpack -d --watch",
    "build": "babel ./app.js -d dist && babel server/ -d dist/server/ && babel server/data/ -d dist/server/data/ && babel server/data/config -d dist/server/data && babel server/controllers/middlewares -d dist/server/controllers/middlewares && babel test/postit.js -d dist/ && babel webpack.config.js -d dist/ --presets es2015",
    "start": "npm run build && nodemon dist/app.js",
    "serve": "node dist/app.js",
    "server": "nodemon dist/app.js",
    "eslint": "./node_modules/.bin/eslint app.js",
    "test": "mocha dist/test --timeout 50000",
    "front-test": "jest",
    "cover": "istanbul cover _mocha dist/test --timeout 10000",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phemmz/PostIt.git"
  },
  "keywords": [
    "Postit"
  ],
  "author": "Adetunji Femi <phemmzmcllroy@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/phemmz/PostIt/issues"
  },
  "homepage": "https://github.com/phemmz/PostIt#readme",
  "dependencies": {
    "axios": "^0.13.1",
    "babel-cli": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.4.1",
    "body-parser": "^1.17.2",
    "classnames": "^2.2.5",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "expect": "^1.20.2",
    "express": "^4.15.3",
    "express-favicon": "^2.0.0",
    "express-session": "^1.15.3",
    "file-loader": "^0.11.2",
    "history": "^4.6.3",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^3.3.1",
    "jest": "^20.0.4",
    "jquery": "^3.2.1",
    "jsonwebtoken": "^7.4.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "morgan": "^1.8.2",
    "multer": "^1.3.0",
    "nock": "^9.0.14",
    "nodemailer": "^4.0.1",
    "pg": "^6.4.0",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-google-login": "^2.9.3",
    "react-redux": "^5.0.5",
    "react-router": "^2.5.2",
    "react-test-renderer": "^15.6.1",
    "redux": "^3.7.1",
    "redux-mock-store": "^1.2.3",
    "redux-thunk": "^2.2.0",
    "request": "^2.81.0",
    "sequelize": "^4.1.0",
    "serve-favicon": "^2.4.3",
    "shortid": "^2.2.8",
    "socket.io": "^2.0.3",
    "superagent": "^3.5.2",
    "supertest": "^3.0.0",
    "validator": "^7.2.0",
